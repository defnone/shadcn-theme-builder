{
  "plugins": ["prettier-plugin-tailwindcss"],


  "singleQuote": true,
  "jsxSingleQuote": true,


  "bracketSameLine": true,


  "tabWidth": 2,
  "printWidth": 100,
  "trailingComma": "es5",
  "semi": true,


  "arrowParens": "avoid",
  "bracketSpacing": true,


  "importOrder": [
    "^@core/(.*)$",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
